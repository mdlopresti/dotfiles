{
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "powershell.scriptAnalysis.enable": true,
    "powershell.integratedConsole.focusConsoleOnExecute": false,
    "powershell.integratedConsole.showOnStartup": false,
    "powershell.startAutomatically": false,
    // "editor.formatOnSave": true,
    "git.defaultCloneDirectory": "~\\source",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "extensions.ignoreRecommendations": false,
    "dev.containers.defaultExtensions": [
        "genieai.chatgpt-vscode"
    ],
    "dotfiles.repository": "https://github.com/mdlopresti/dotfiles.git",
    "editor.suggestSelection": "first",
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "liveshare.connectionMode": "relay",
    "diffEditor.ignoreTrimWhitespace": false,
    "explorer.confirmDelete": false,
    "files.associations": {
        ".yamllint": "yaml",
        ".ansible-lint": "yaml",
        "Vagrantfile": "ruby",
        "*.yml": "yaml",
        "*.rb": "ruby",
        ".textlintrc": "json"
    },
    "editor.fontFamily": "SauceCodePro Nerd Font, 'Courier New', monospace",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "editor.largeFileOptimizations": false,
    "git.ignoreRebaseWarning": true,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "window.zoomLevel": 2,
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "task.allowAutomaticTasks": "on",
    "snyk.yesWelcomeNotification": false,
    "python.analysis.extraPaths": [
        "${workspaceFolder}"
    ],
    "aws.telemetry": false,
    "pymakr.devices.configs": {
        "0001": {
            "autoConnect": "always",
            "username": "micro",
            "password": "python",
            "hidden": false,
            "rootPath": "/",
            "adapterOptions": {}
        }
    },
    "pymakr.misc.notifications": {
        "Uploading a project will delete all existing files on the device before uploading the project folder. After uploading a project, you can start it by restarting the device. For faster uploads without file deletion, please put the device in dev mode.": "Don't show again"
    },
    "CodeGPT.apiKey": "OpenAI"
}