{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "#26C6DA",
          "background": "#000000",
          "properties": {
            "macos": "mac"
          }
        },
        {
          "type": "session",
          "style": "powerline",
          "foreground": "#000000",
          "background": "#00FF00",
          "leading_diamond": "\ue0b6"
        },
        {
          "type": "shell",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#0077c2"
        },
        {
          "type": "az",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#000033",
          "properties": {
            "display_name": true
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#61AFEF",
          "properties": {
            "style": "mixed"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "spotify",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#1BD760",
          "properties": {
            "paused_icon": "\uf8e3 ",
            "playing_icon": "\ue602 ",
            "stopped_icon": "\uf04d ",
            "track_separator": ":"
          }
        }
      ]
    }
  ]
}